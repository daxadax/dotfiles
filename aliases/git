alias gst='git status'
alias ga='git add'
alias gc='git commit -v'
alias gco='git checkout'
alias gba='git branch -a'
alias gd='git diff'
alias stash='git stash'
alias pop='git stash pop'
alias gl='git pull -v'
alias gp='git push -v'
alias gg='git prettylog'
alias gfiles='git whatchanged --oneline --name-only | grep --extended-regexp --invert-match '^[[:xdigit:]]{7,}' | head -n20'
